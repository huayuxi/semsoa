@charset "utf-8";
/*controlFrm CSS Document */
body { background:url(../images/contractionbar_bg.gif) repeat-y;}

#controlblock_btn{
	width:4px;
   	height:101px;
	position:absolute;
	left:0;
	top:44%;
	display:block;
  	cursor:pointer;
	}
#controlblock_btn.show,.show
{
    background:url(../themes/orange/images/btn_collapsible_close.jpg) no-repeat;
}

#controlblock_btn.hidden,.hidden
{
   background:url(../themes/orange/images/btn_collapsible_open.jpg) no-repeat;
}

