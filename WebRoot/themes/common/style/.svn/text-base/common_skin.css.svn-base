@charset "utf-8";
/*orange_skin CSS Document */
*{
    margin:0;
	padding:0;
}

html, body {
	margin:0;
	padding:0;
	background: #ffffff;
}
a:link,
a:visited{color:#2b89bf;text-decoration:none;}
a:hover{color:#f29000;text-decoration:underline;}

/*page*/
.layout {
	margin:0 auto;
	padding: 0;
	width: 100%;
	min-width:956px;
	color:#888;
    background:#fff;
	position:relative;
}
.container,
.r_main{ padding:10px;}

/*----------------------link_style-----------------------*/
a.link_sty1{}
a.link_sty1:hover{}

/*----------------------font_style-----------------------*/
.font_color_f56a0b{ color:#f56a0b;}
.font_color_434343{ color:#434343;}
.font_color_f39700{ color:#f39700;}
.font_color_22ac38{ color:#22ac38;}
.font_color_898989{ color:#898989;}
.font_color_a0a0a0{ color:#a0a0a0;}
.font_red{ color:#F00;}
.font_bold{	font-weight:bold;} 

/*----------------------text_input_style-----------------------*/
.input_text_sty1{ height:18px; line-height:18px; border:1px solid #b5b5b5; }
.input_text_sty_22{ height:22px; line-height:22px; border:1px solid #b5b5b5; }
.input_text_sty_26{ height:26px; line-height:26px; border:1px solid #b5b5b5; }
.input_text_sty_border{ border:1px solid #b5b5b5; }
.input_text_dim{ background:#dedede;}

/*-----------------------button_style-------------------------*/
.button{ padding:0 10px; border:0; height:28px; background:url(../images/btn01_1x28.gif) repeat-x; color:#FFF; cursor:pointer;}
.button:hover{ padding:0 10px; border:0; height:28px; background:#f29000; color:#FFF; cursor:pointer;}
.button:active{ padding:0 10px; border:0; height:28px; background:#FCAA2D; color:#FFF; cursor:pointer;}

.button_blue{ padding:0 10px; border:0; height:28px; background:url(../images/btn02_1x28.gif) repeat-x; color:#FFF; cursor:pointer;}
.button_blue:hover{ padding:0 10px; border:0; height:28px; background:#2d98b7; color:#FFF; cursor:pointer;}
.button_blue:active{ padding:0 10px; border:0; height:28px; background:#52BFDF; color:#FFF; cursor:pointer;}

.button_small{ padding:0 10px; border:0; height:22px; background:url(../images/btn01_1x22.gif) repeat-x; color:#FFF; cursor:pointer;}
.button_small:hover{ padding:0 10px; border:0; height:22px; background:#f29000; color:#FFF; cursor:pointer;}
.button_small:active{ padding:0 10px; border:0; height:22px; background:#FCAA2D; color:#FFF; cursor:pointer;}

.btn_dim,
.btn_dim:hover,
.btn_dim:active{ padding:0 10px; border:0; background:#b5b5b5; color:#FFF;}

/*-------------------------list_style---------------------------*/
.t_list_info{ background:#ffffff;}
.t_list_info th,.t_list_info td{ padding:3px 5px; color:#434343; line-height:150%; border-right:1px #ffffff solid; text-align:center;}
.t_list_info th{ background:#efefef; color:#434343;}
.odd{ background:#F4FBF4;}
.even{ background:#ffffff;}
.highlight{ background:#dbeeff;}
td.list_result{ background:#DBEEFF; text-align:left;} 

/*----------------------- form style -------------------------*/
.form_block {padding:10px 24px; }

.t_form01 td{ padding:3px 2px; }
.t_form01 td.propertyName { text-align: left; color: #333333; }
.t_form01 td.propertyInfo { text-align: left; color: #434343;}

.t_form_view td{ padding:5px 3px; vertical-align:top;}
.t_form_view td.propertyName { text-align: right; color: #555555; font-weight:bold;}
.t_form_view td.propertyInfo { text-align: left; color: #434343;}

.t_form_view_i td{ padding:7px 3px; vertical-align:top;}
.t_form_view_i td.propertyName { text-align: right; color: #555555; font-weight:bold;}
.t_form_view_i td.propertyInfo { text-align: left; color: #434343;}
.goods_img{ height:200px; width:200px; float:left; border:1px solid #A0A0A0;}
.goods_feature{ clear:both; padding-top:20px; width:750px;}
.form_view_i{ float:left; width:360px; padding-left:40px;}
input.ckbox{ vertical-align:middle; margin:-2px 4px 0 0;}


/*---------------------------------------breadcrumb----------------------------------------*/
.breadcrumb{
	font: normal 12px "宋体";
	color:#696969;
	overflow:hidden;
	line-height:20px;
	/*background:#efefef;  set background*/
}
.breadcrumb span{
	display:inline-block;
	padding:0 20px 0 0;
	background:url(../../../images/vane.gif) right center no-repeat;
}
.breadcrumb span a,
.breadcrumb span a:link{
	color:#0f64a6;
	text-decoration:none;
}
.breadcrumb span a:hover{
	color:#ff4e00;
	text-decoration:underline;
}
.breadcrumb span.firstLi{
	padding-right:0;
	background:none;
}
.breadcrumb span.urhere{
	color:#F56A0B;
	font-weight:bold;
	padding:0 5px;
	background:none;
}

/*---------------------------------------search_form----------------------------------------*/
.search { border: #eeeeee 1px solid; padding: 10px; margin:5px 0;}
.search td { color:#666666;}
.search td label{ padding:4px;}

/*---------------------------------------progress_bar----------------------------------------*/
.progressBar {
	width:250px;
	text-align:left;
}

/*---------------------------------------steps_bar----------------------------------------*/
.reg_step{
	margin:0 auto;
	padding:10px 0;
	height:33px;
	font: normal 12px "Lucida Grande", Arial, Lucida, Verdana, Helvetica, "宋体", sans-serif; 
}
.reg_step ul{
	margin:0;
	padding:0;
}
.reg_step li{
	float:left;
	height:33px;
	line-height:33px;
	text-align:left;
	font-size:14px;
	font-weight:bold;
}
.reg_step li span{
	height:33px;
	line-height:33px;
	color:#999;
	float:left;
	padding-left:30px;
	font-size:14px;
	font-weight:bold;
	background:url(../images/step_num.png) no-repeat;
}
.step0{
	width:170px;
	text-indent:2.5em;
	color:#707070;
	background: transparent url(../images/step_head_bg.png) 0 0 no-repeat;
}
.step1, .step2{
	width:196px;
	text-indent:2em;
	color:#999;
	margin-left:-20px;
	background:url(../images/step_bg.png) right 0 no-repeat;
}
.step3{
	width:196px;
	text-indent:2em;
	color:#999;
}
.reg_step li.step1 span{
	background-position:30px 5px;

}
.reg_step li.step1 span.on{
	background-position:30px -25px;
    color:#F60;
}

.reg_step li.step2 span{
	background-position:30px -50px;
}
.reg_step li.step2 span.on{
	background-position:30px -80px;
    color:#F60;
}

.reg_step li.step3 span{
	background-position:30px -107px;
	width:70px;
}
.reg_step li.step3 span.on{
	background-position:30px -135px;
	color:#F60;
}

.reg_step li.step4 span{
	background-position:30px -150px;
}
.reg_step li.step4 span.on{
	background-position:30px -175px;
	color:#F60;
}

