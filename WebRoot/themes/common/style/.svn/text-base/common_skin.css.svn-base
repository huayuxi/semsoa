/*pagenum_a---------------------*/
.pagenum_a{
	font: normal 12px "宋体";
	color:#696969;
	display:block;
	width:100%;
	height:20px;
	line-height:20px;
	padding:10px 0;
}
.pagenum_a input{
	width:50px;
	margin:0 5px;
	line-height:16px;
	height:16px;
	vertical-align:middle;
}
.pagenum_a a,
.pagenum_a a:link,
.pagenum_a a:visited {
	border:1px solid #dadada;
	color:#696969;
	font-size:12px;
	padding:1px 5px 3px;
	text-decoration:none;
	line-height:20px;*line-height:13px;_line-height:15px;
	vertical-align:middle;
}
.pagenum_a a:hover,
.pagenum_a a:active {
	background:#FCAA2D;
	border:1px solid #FCAA2D;
	color:#fff;
	padding:1px 5px 3px;
	text-decoration:none;
	line-height:20px;*line-height:13px;_line-height:15px;
	vertical-align:middle;
}
.pagenum_a a.current,
.pagenum_a a.current:link
.pagenum_a a.current:visited{
	border-color:#fff;
	color:#ccc;
	padding:1px 5px 3px;
	background:none;
	line-height:20px;*line-height:13px;_line-height:15px;
	vertical-align:middle;
}
.pagenum_a .backward_dim a,
.pagenum_a .backward_dim a:hover,
.pagenum_a .backward_dim a:active,
.pagenum_a .forward_dim a,
.pagenum_a .forward_dim a:hover,
.pagenum_a .forward_dim a:active{
	color:#ccc;
	background:none;
	border-color:#dadada;
	cursor:default;
}
.pagenum_a .backward a,
.pagenum_a .backward a:hover,
.pagenum_a .backward a:active,
.pagenum_a .forward a,
.pagenum_a .forward a:hover,
.pagenum_a .forward a:active{
/*可扩展*/
}
.pagenum_a .page_ipt{/*可扩展*/}
.pagenum_a .btn_s{
	width:40px;
	height:20px;
	line-height:20px;*line-height:13px;_line-height:15px;
	text-align:center;
	vertical-align:middle;
	/* input 元素type=button 情形之下，文字垂直居中的hack */
	font-family:Verdana;*font-family:Georgia;_font-family:Tahoma;
	padding:0 5px 1px;*padding:3px 2px 1px;_padding:0 3px 1px;
}

