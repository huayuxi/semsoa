/*---------------------------------------tab----------------------------------------*/
/* 可扩展长度标签*/
div.movarea {
    /* Set it so we could calculate the offsetLeft */
    position: relative;
    height: 30px;
}
ul.movarea {
    display: block;
    height: 28px;
    /* max width here, for users without javascript */   
    list-style: none;
	border-bottom:2px solid #FF930B;
	padding-left:5px;
}
.movarea li {
    display: inline-block;/*IE6*/
	height:28px;
    float: left;
	margin-right:2px;
	padding:0;
	font-weight:bold;
}
.movarea li a {
	display: inline-block;
    float:left;
	background:url(../images/tableft1.gif) left top no-repeat;
	margin:0;
	padding:0 0 0 4px;
	text-decoration:none;
	color:#888;
	height:28px;
}
.movarea li a span {
    float:left;
	display:inline-block;
	background:url(../images/tabright1.gif) right top no-repeat;
	padding:6px 10px 2px 6px;
	margin:0;
    text-align: center;
    font-size: 12px;   
	height:20px;
}
.movarea li a:hover {
	display: inline-block;
	color:#333;
	text-decoration:none;
}


.movarea li a:hover span {
    display: inline-block;
    color:#333;
}
/* Commented Backslash Hack hides rule from IE5-Mac \*/
ul.movarea li a span {
	float:none;
}
/* End IE5-Mac hack */
.movarea li.focus a {
	display: inline-block;
	background-position:0% -42px;
	text-decoration:none;
}
.movarea li.focus a span{ 
	display: inline-block;
    color:#fff;
	background-position:100% -42px;
	}
	
